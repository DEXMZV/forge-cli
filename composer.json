{
    "name": "sven/forge-cli",
    "description": "A set of commands to interact with your Laravel Forge servers & sites",
    "keywords": ["laravel", "forge", "command", "cli"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "svenluijten1995@gmail.com",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "symfony/console": "~2.3|~3.0",
        "symfony/process": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Sven\\ForgeCLI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\ForgeCLI\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/forge"
    ]
}
